  </template>
  <template name="tag" value="private static final String TAG = $CLASS_NAME$.class.getSimpleName();$END$" description="Inserts psf TAG" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="true" />
